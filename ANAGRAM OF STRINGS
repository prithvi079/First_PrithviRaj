#include<iostream>
using namespace std;
class student 
{
  private:
  string first-name;
  string last-name;
  Public:
  void set-name(string f,string l)
  {
    first-name =f;
    last-name =l;
  }
  first void check-name(student k);
};
void check-name(student k)
{
  string s1 = k.firstname;
  string s2 = k.lastname;
  sort(s1.begin(),s1.end());
  sort(s2.begin(),s2.end());
  if(s1=s2)
  {
    cout<<"ANANGRAM"<<endl;
  else
  {
    cout<<"NOT ANANGRAM"<<endl;
  }
  
  




